library(xtable)
library(knitr)
library(kableExtra)
library(tidyverse)
table_df <- data.frame(
world     = c("Berrone 2016"),
direction = c(linebreak("Lags\nAutocorrelation\nOther")),
model     = c(linebreak("Initial Conditions\nTimescales\nBoundary Conditions"))
)
names(table_df) <- c("Article", "Dynamic Notions", "Opportunities")
View(table_df)
apa_table(
table_df
, caption = "Structure of Monte Carlo simulations. Within each case, true causal lags varied from zero to five."
, note      = "In all conditions $b_1$ is the focal parameter estimate."
, escape = FALSE
)
